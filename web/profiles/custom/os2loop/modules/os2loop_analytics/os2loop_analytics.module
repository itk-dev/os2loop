<?php

/**
 * @file
 * Hook implementations of os2loop_analytics module.
 */

/**
 * Implements hook_page_attachments().
 *
 * Set analytics tools in head.
 */
function os2loop_analytics_page_attachments(array &$attachments) {
  Drupal::service('os2loop_analytics.analytics_helper')->setAnalytics($attachments);
}
